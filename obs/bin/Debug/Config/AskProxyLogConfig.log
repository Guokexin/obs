log4j.appender.AgentLockLog.layout.ConversionPattern=%d{ISO8601} %5p %c{1}:%L - %m%n


log4j.rootLogger=main
log4j.logger.ReportLog = INFO,ReportLog
log4j.logger.PBXDATALOG = INFO,PBXDATALOG
log4j.logger.AskProxy = INFO,AskProxy
log4j.logger.keepalive = INFO,keepalive
log4j.logger.AgentAMI = INFO,AgentAMI
log4j.logger.SkillSnap = INFO,SkillSnap
log4j.logger.CheckAgentLog = INFO,CheckAgentLog
log4j.logger.AgentLockLog = INFO,AgentLockLog



