log4j.appender.AskProxy.File=./log/AskProxy.log
log4j.appender.AskProxy.MaxFileSize=5MB
log4j.appender.AskProxy.MaxBackupIndex=10
log4j.appender.AskProxy.layout=org.apache.log4j.PatternLayout 
log4j.appender.AskProxy.layout.ConversionPattern=%d{ISO8601} %5p %c{1}:%L - %m%n

# keepalive logfile
