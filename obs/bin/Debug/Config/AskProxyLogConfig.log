#AgentLockLog logfile
log4j.appender.AgentLockLog=org.apache.log4j.RollingFileAppender
log4j.appender.AgentLockLog.File=./log/AgentLockLog.log
log4j.appender.AgentLockLog.MaxFileSize=5MB
