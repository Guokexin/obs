log4j.appender.PBXDATALOG.MaxBackupIndex=10
log4j.appender.PBXDATALOG.layout=org.apache.log4j.PatternLayout 
log4j.appender.PBXDATALOG.layout.ConversionPattern=%d{ISO8601} %5p %c{1}:%L - %m%n

# AskProxy logfile
