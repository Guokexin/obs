log4j.appender.AgentAMI.layout=org.apache.log4j.PatternLayout 
log4j.appender.AgentAMI.layout.ConversionPattern=%d{ISO8601} %5p %c{1}:%L - %m%n

# SkillSnap logfile
log4j.appender.SkillSnap=org.apache.log4j.RollingFileAppender
log4j.appender.SkillSnap.File=./log/SkillSnap.log
log4j.appender.SkillSnap.MaxFileSize=5MB
log4j.appender.SkillSnap.MaxBackupIndex=10
log4j.appender.SkillSnap.layout=org.apache.log4j.PatternLayout 
