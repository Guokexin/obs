log4j.appender.keepalive.MaxFileSize=5MB
log4j.appender.keepalive.MaxBackupIndex=10
log4j.appender.keepalive.layout=org.apache.log4j.PatternLayout 
log4j.appender.keepalive.layout.ConversionPattern=%d{ISO8601} %5p %c{1}:%L - %m%n

# AgentAMI logfile
