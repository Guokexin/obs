# CheckAgentLog logfile
log4j.appender.CheckAgentLog=org.apache.log4j.RollingFileAppender
log4j.appender.CheckAgentLog.File=./log/CheckAgentLog.log
log4j.appender.CheckAgentLog.MaxFileSize=5MB
